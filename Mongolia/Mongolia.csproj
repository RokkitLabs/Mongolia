<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Mongolia</Title>
        <Description>Mongolia Poggers</Description>
        <PackageProjectUrl>https://github.com/EpicTestingTempOrganizationForStuff/Mongolia</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/EpicTestingTempOrganizationForStuff/Mongolia/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/EpicTestingTempOrganizationForStuff/Mongolia</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <PackageTags>ORM MONGODB</PackageTags>
        <NeutralLanguage>en-GB</NeutralLanguage>
        <PackageIconUrl>https://raw.githubusercontent.com/EpicTestingTempOrganizationForStuff/Mongolia/master/logo.png</PackageIconUrl>
        <PackageVersion>1.0.1</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MongoDB.Bson" Version="2.11.5" />
      <PackageReference Include="MongoDB.Driver" Version="2.19.0" />
      <PackageReference Include="MongoDB.Driver.GridFS" Version="2.11.5" />
    </ItemGroup>

</Project>
